{
  "name": "buttons",
  "version": "1.0.0",
  "description": "add buttons to your obsidian vault",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "cp-watch": "fswatch -0 main.js | xargs -0 -I {} cp main.js ~/plugin-development/.obsidian/plugins/buttons/",
    "cp": "cp main.js ~/plugin-development/.obsidian/plugins/buttons/",
    "test": "jest",
    "test:watch": "jest --watch --coverage=false",
    "eleventy": "npx @11ty/eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@babel/core": "^7.19.3",
    "@types/jest": "27.x",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "builtin-modules": "^3.2.0",
    "esbuild": "^0.13.12",
    "eslint": "^8.25.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-react": "^7.31.1",
    "jest": "27.x",
    "jest-environment-jsdom": "27.x",
    "jest-preset-preact": "^4.0.5",
    "obsidian": "^0.16.0",
    "preact-render-to-string": "5.x",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "27.1.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@codemirror/language": "^6.2.1",
    "@codemirror/state": "^6.1.1",
    "@codemirror/view": "^6.2.0",
    "esbuild-plugin-inline-worker": "^0.1.1",
    "preact": "^10.10.6",
    "react-select": "^5.4.0"
  }
}
